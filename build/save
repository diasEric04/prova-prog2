while (!routes.empty()) {
        auto currentRoute = routes.front();
        routes.erase(routes.begin());
        node = currentRoute.back();

        bool fNoMoreRoutes = false;

        auto neighbors = node->getNeighbors();
        for (auto neighbor : neighbors) {
            auto visit = visited.begin();
            while (visit != visited.end()) {
                if (! (*visit == neighbor->getName())) {
                    visited.push_back(neighbor->getName());
                    std::vector<City*> newRoute = currentRoute;
                    newRoute.push_back(neighbor);
                    routes.push_back(newRoute);
                }
                visit++;
                if (visit == visited.end()) {
                    fNoMoreRoutes = true;
                }
            }
            break;
        }
        if (fNoMoreRoutes) {
            break;
        }
    }
